{
	"name": "destiny-launcher",
	"private": true,
	"proxy": "https://dev.destinylauncher.net/",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --force -dx --exclude node_modules/ --exclude .env.local --exclude .env.*.local",
		"dev": "next dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"postinstall": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path ./.gitignore .",
		"lint:prettier": "prettier --check --ignore-path ./.gitignore .",
		"lint:typescript": "tsc",
		"start": "next start"
	},
	"lint-staged": {
		"*": "prettier --write --ignore-unknown",
		"*.{js,ts,tsx}": "eslint --fix"
	},
	"dependencies": {
		"@dnd-kit/core": "^3.0.3",
		"@dnd-kit/sortable": "^3.0.1",
		"@dnd-kit/utilities": "^2.0.0",
		"@mdx-js/loader": "^1.6.22",
		"@mdx-js/react": "^1.6.22",
		"@next/bundle-analyzer": "^10.2.3",
		"@next/mdx": "^10.2.3",
		"@tailwindcss/aspect-ratio": "^0.2.1",
		"@types/gtag.js": "^0.0.4",
		"@types/workbox-window": "^4.3.3",
		"autoprefixer": "^10.2.5",
		"babel-plugin-optimize-clsx": "^2.6.1",
		"bungie-api-ts": "^4.9.0",
		"clsx": "^1.1.1",
		"focus-visible": "^5.2.0",
		"framer-motion": "^4.1.17",
		"gray-matter": "^4.0.3",
		"next": "^10.2.3",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^3.0.2",
		"next-pwa": "^5.2.21",
		"next-transpile-modules": "^7.2.0",
		"postcss-import": "^14.0.2",
		"postcss-preset-env": "^6.7.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.2.0",
		"sass": "^1.34.0",
		"tailwindcss": "^2.1.2"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "^10.2.3",
		"@types/mdx-js__react": "^1.5.3",
		"@types/node": "^15.6.1",
		"@types/react": "^17.0.8",
		"@types/react-dom": "^17.0.5",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.27.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-etc": "^1.4.0",
		"eslint-plugin-import": "^2.23.3",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-etc": "^1.0.7",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.0",
		"prettier-plugin-tailwind": "^2.2.10",
		"shx": "^0.3.3",
		"stylelint": "^13.13.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-recommended": "^5.0.0",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-order": "^4.1.0",
		"stylelint-prettier": "^1.2.0",
		"stylelint-scss": "^3.19.0",
		"typescript": "^4.2.4",
		"typescript-plugin-css-modules": "^3.2.0"
	}
}
