{
	"name": "destiny-launcher",
	"private": true,
	"version": "3.4.4",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --force -dx --exclude node_modules/ --exclude .env.local --exclude .env.*.local",
		"dev": "next dev",
		"https": "local-ssl-proxy --source 3001 --target 3000",
		"devs": "npm-run-all --parallel https dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"postinstall": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path ./.gitignore .",
		"lint:prettier": "prettier --check --ignore-path ./.gitignore .",
		"lint:typescript": "tsc",
		"start": "next start"
	},
	"nano-staged": {
		"*": "prettier --write --ignore-unknown",
		"*.{ts,tsx,js,jsx}": "eslint --fix"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.0.5",
		"@dnd-kit/sortable": "^7.0.1",
		"@dnd-kit/utilities": "^3.2.0",
		"@mdx-js/loader": "^2.1.2",
		"@mdx-js/react": "^2.1.2",
		"@next/bundle-analyzer": "^12.2.3",
		"@next/mdx": "^12.2.3",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tsconfig/next": "^1.0.2",
		"@types/gtag.js": "^0.0.10",
		"@types/workbox-window": "^4.3.4",
		"autoprefixer": "^10.4.8",
		"babel-plugin-optimize-clsx": "^2.6.2",
		"bungie-api-ts": "^4.18.0",
		"clsx": "^1.2.1",
		"cookie": "^0.5.0",
		"focus-visible": "^5.2.0",
		"framer-motion": "^6.5.1",
		"gray-matter": "^4.0.3",
		"next": "^12.2.3",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^4.1.0",
		"next-pwa": "^5.5.5",
		"next-transpile-modules": "^9.0.0",
		"plaiceholder": "^2.4.0",
		"postcss-import": "^14.1.0",
		"postcss-preset-env": "^7.7.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.4.0",
		"sass": "^1.54.0",
		"sharp": "^0.30.7",
		"swr": "^1.3.0",
		"tailwindcss": "^3.1.7"
	},
	"devDependencies": {
		"@tsconfig/next": "^1.0.2",
		"@types/cookie": "^0.5.1",
		"@types/mdx-js__react": "^1.5.5",
		"@types/node": "^18.6.3",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/umami": "^0.1.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.20.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^12.2.3",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-etc": "^2.0.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^8.0.1",
		"nano-staged": "^0.8.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"shx": "^0.3.4",
		"typescript": "^4.7.4",
		"typescript-plugin-css-modules": "^3.4.0"
	}
}
