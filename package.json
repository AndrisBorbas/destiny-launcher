{
	"name": "destiny-launcher",
	"private": true,
	"version": "3.6.2",
	"packageManager": "pnpm@8.3.1",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --force -dx --exclude node_modules/ --exclude .env.local --exclude .env.*.local",
		"dev": "next dev",
		"https": "local-ssl-proxy --source 3001 --target 3000",
		"devs": "npm-run-all --parallel https dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path ./.gitignore .",
		"lint:prettier": "prettier --check --ignore-path ./.gitignore .",
		"lint:typescript": "tsc",
		"start": "next start"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/sortable": "^8.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@mdx-js/loader": "^2.3.0",
		"@mdx-js/react": "^2.3.0",
		"@next/bundle-analyzer": "^12.3.4",
		"@next/mdx": "^12.3.4",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tsconfig/next": "^1.0.5",
		"@types/workbox-window": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"babel-plugin-optimize-clsx": "^2.6.2",
		"bungie-api-ts": "^5.1.0",
		"clsx": "^2.1.1",
		"cookie": "^0.7.1",
		"focus-visible": "^5.2.1",
		"framer-motion": "^11.11.1",
		"gray-matter": "^4.0.3",
		"next": "^12.3.4",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^4.4.1",
		"next-pwa": "5.6.0",
		"plaiceholder": "^3.0.0",
		"postcss-import": "^16.1.0",
		"postcss-preset-env": "^10.0.6",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"sass": "^1.79.4",
		"sharp": "^0.33.5",
		"swr": "^2.2.5",
		"tailwindcss": "^3.4.13"
	},
	"devDependencies": {
		"@tsconfig/next": "^1.0.5",
		"@types/cookie": "^0.6.0",
		"@types/mdx-js__react": "^1.5.8",
		"@types/node": "^18.19.54",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.0",
		"@types/umami": "^0.1.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-next": "^12.3.4",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.0",
		"eslint-plugin-react": "^7.37.1",
		"eslint-plugin-react-etc": "^2.0.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"typescript": "^5.6.2",
		"typescript-plugin-css-modules": "^5.1.0"
	}
}
